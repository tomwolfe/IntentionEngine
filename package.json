{
  "name": "intentionengine",
  "version": "1.0.0",
  "description": "IntentionEngine - High-Risk AI Infrastructure",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:phase0": "jest tests/phase0_tests.ts",
    "test:phase1": "jest tests/phase1_tests.ts",
    "test:phase2": "jest tests/phase2_tests.ts",
    "test:phase3": "jest tests/phase3_tests.ts",
    "test:phase4": "jest tests/phase4_tests.ts",
    "test:phase5": "jest tests/phase5_tests.ts",
    "test:all": "jest tests/",
    "test:integration": "jest tests/integration_tests.ts"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/*.test.ts"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "core/**/*.ts",
      "tests/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomwolfe/IntentionEngine.git"
  },
  "keywords": [
    "intent",
    "ai",
    "deterministic",
    "reproducible"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/tomwolfe/IntentionEngine/issues"
  },
  "homepage": "https://github.com/tomwolfe/IntentionEngine#readme",
  "dependencies": {
    "@ai-sdk/openai": "^3.0.25",
    "@ai-sdk/react": "^3.0.71",
    "@upstash/redis": "^1.36.2",
    "ai": "^6.0.69",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
